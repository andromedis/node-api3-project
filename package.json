{
    "name": "node-api3-project",
    "version": "1.0.0",
    "scripts": {
        "start": "node index.js",
        "server": "nodemon index.js",
        "dev": "ts-node-dev --respawn --pretty --transpile-only index.ts",
        "ts-start": "ts-node index.ts",
        "test": "cross-env NODE_ENV=testing jest --watch --verbose --runInBand --silent",
        "resetdb": "knex migrate:rollback && knex migrate:latest && knex seed:run"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/LambdaSchool/node-api3-project.git"
    },
    "dependencies": {
        "express": "^4.17.1",
        "knex": "^0.95.4",
        "sqlite3": "^5.0.2"
    },
    "devDependencies": {
        "@types/eslint": "^8.4.2",
        "@types/express": "^4.17.13",
        "@types/jest": "^26.0.22",
        "@types/knex-cleaner": "^1.3.2",
        "@types/node": "^17.0.35",
        "@types/nodemon": "^1.19.1",
        "@types/sqlite3": "^3.1.8",
        "@types/supertest": "^2.0.12",
        "cross-env": "^7.0.3",
        "eslint": "^7.23.0",
        "jest": "^26.6.3",
        "knex-cleaner": "^1.3.1",
        "nodemon": "^2.0.7",
        "supertest": "^6.1.3",
        "ts-node": "^10.8.0",
        "ts-node-dev": "^1.1.8",
        "typescript": "^4.7.2"
    }
}
